    <validationRules>
        <fullName>BMCServiceDesk__Cannot_Update_Closed_Change_Request</fullName>
        <active>true</active>
        <description>This Rule does not allow user to modify record, if it is closed.</description>
        <errorConditionFormula>AND( NOT( BMCServiceDesk__State__c ) , NOT(PRIORVALUE( BMCServiceDesk__State__c ) ), NOT( ISNEW() ),NOT( ISCHANGED( BMCServiceDesk__Total_Duration_minutes__c ) )  )</errorConditionFormula>
        <errorMessage>This change request is closed. Reopen the change request to modify it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__Check_ActualEndDate</fullName>
        <active>true</active>
        <description>This rule checks to make sure that Actual End Date comes always after Actual Start Date.</description>
        <errorConditionFormula>AND(NOT(OR(ISNULL(BMCServiceDesk__Actual_End_Date__c),ISBLANK(BMCServiceDesk__Actual_End_Date__c) ) ),OR( BMCServiceDesk__Actual_End_Date__c &lt; BMCServiceDesk__Actual_Start_Date__c,  BMCServiceDesk__Actual_End_Date__c = BMCServiceDesk__Actual_Start_Date__c) )</errorConditionFormula>
        <errorMessage>Actual End Date must be later than Actual Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__Check_ReviewDate</fullName>
        <active>true</active>
        <description>This rule checks to make sure that Review Date comes always after Actual End Date.</description>
        <errorConditionFormula>AND(NOT(OR(ISNULL(BMCServiceDesk__Review_Date__c),ISBLANK(BMCServiceDesk__Review_Date__c) ) ),OR( BMCServiceDesk__Review_Date__c &lt; BMCServiceDesk__Actual_End_Date__c,  BMCServiceDesk__Actual_End_Date__c = BMCServiceDesk__Review_Date__c) )</errorConditionFormula>
        <errorMessage>The Review Date must be later than the Actual End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__Check_ScheduledEndDate</fullName>
        <active>true</active>
        <description>This rule checks to make sure that Scheduled End Date comes always after Schedule Start Date.</description>
        <errorConditionFormula>AND(NOT(OR(ISNULL(BMCServiceDesk__Scheduled_End_Date__c),ISBLANK(BMCServiceDesk__Scheduled_End_Date__c) ) ), BMCServiceDesk__Scheduled_End_Date__c &lt; BMCServiceDesk__Scheduled_Start_Date__c )</errorConditionFormula>
        <errorMessage>Scheduled End Date must be later than Scheduled Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__Check_ScheduledStartDate</fullName>
        <active>true</active>
        <description>This rule checks to make sure that Actual End Date comes always after Scheduled Start Date.</description>
        <errorConditionFormula>AND(NOT(OR(ISNULL(BMCServiceDesk__Scheduled_Start_Date__c),ISBLANK(BMCServiceDesk__Scheduled_Start_Date__c) ) ),OR( BMCServiceDesk__Actual_End_Date__c &lt; BMCServiceDesk__Scheduled_Start_Date__c, BMCServiceDesk__Actual_End_Date__c = BMCServiceDesk__Scheduled_Start_Date__c) )</errorConditionFormula>
        <errorMessage>The Actual End Date must be later than the Scheduled Start Date</errorMessage>
    </validationRules>
