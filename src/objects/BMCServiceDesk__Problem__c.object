        <errorConditionFormula>AND( NOT( BMCServiceDesk__State__c ) , NOT( PRIORVALUE( BMCServiceDesk__State__c ) ),NOT( ISNEW() ), NOT( ISCHANGED( BMCServiceDesk__Total_Duration__c ) ), NOT( ISCHANGED( BMCServiceDesk__Number_of_linked_Changes__c ) ), NOT( ISCHANGED( BMCServiceDesk__Number_of_linked_Incidents__c ) )   )</errorConditionFormula>
