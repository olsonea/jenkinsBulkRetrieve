<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <validationRules>
        <fullName>BMCServiceDesk__aPriorityID_Required</fullName>
        <active>true</active>
        <errorConditionFormula>OR(OR(ISBLANK(Name), ISNULL(Name)), NOT(ISNUMBER(Name)))</errorConditionFormula>
        <errorMessage>Please enter a numeric value in the Priority field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__cImpact_ID_Required</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISBLANK( BMCServiceDesk__impact_Id__c ) , ISNULL( BMCServiceDesk__impact_Id__c ) )</errorConditionFormula>
        <errorMessage>Please enter Impact.</errorMessage>
    </validationRules>
</CustomObject>
