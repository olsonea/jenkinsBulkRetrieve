function detectIE() {
  var ua = window.navigator.userAgent;
  var msie = ua.indexOf('MSIE ');
  if (msie > 0) {
    // IE 10 or older => return version number
    return true;
  }
  
  //for IE 11
  if(ua.indexOf("Trident/7.0") > 0)
      return true;

  //for IE Edge and other browsers
  return false;
}

var ScanId = '';
