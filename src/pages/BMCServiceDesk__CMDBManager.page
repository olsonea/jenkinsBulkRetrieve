                var success_message = '{!JSENCODE(success)}';  
                if(success_message == 'success'){
                  
                }
                else{
                     Ext.MessageBox.show({
                       
                        title: '{!JSENCODE($Label.Error)}',
                        msg: success_message,
                        icon: Ext.MessageBox.ERROR,
                        buttons: Ext.MessageBox.OK
                    });
                } 
            }
