                function showInfoMessage(errorMessage ){
                    var msgTxt = '';
                    if(errorMessage != ''){
                        msgTxt = errorMessage ;
                    }else{
                        msgTxt = '{!JSENCODE($Label.CountUpdateBatchSubmitted)}';
                    }
                    Ext.MessageBox.show({
                        msg: msgTxt  ,
                        title: '{!JSENCODE($Label.Information)}',
                        width:300,
                        height:'auto',
                        buttons: Ext.MessageBox.OK
                    });
                }
